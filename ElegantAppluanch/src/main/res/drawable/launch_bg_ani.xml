<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="newApi"
    android:drawable="@drawable/launch_bg_001">
    <target android:name="dark">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M0,0 L900,0 L900,800 L0,300  L0,0 Z"
                android:valueTo   =  "M0,0 L900,0 L900,450 L0,1000 L0,0 Z"
                android:duration="3000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
    <target android:name="light">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M0,400 L900,1400 L900,1600 L0,1600 L0,1400 Z"
                android:valueTo   =  "M0,1400 L900,100 L900,1600 L0,1600 L0,1400 Z"
                android:duration="3000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
    <target android:name="one">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M125,400 l50,0 l0,500 l-50,0 l0,-500 Z"
                android:valueTo   =  "M225,440 l50,0 l0,260 l-50,0 l0,-260 Z"
                android:duration="3000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
    <target android:name="two">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M275,500 l50,0 l0,300 l-50,0 l0,-300 Z"
                android:valueTo   =  "M325,540 l50,0 l0,160 l-50,0 l0,-160 Z"
                android:duration="2000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
    <target android:name="three">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M425,600 l50,0 l0,100 l-50,0 l0,-100 Z"
                android:valueTo   =  "M425,600 l50,0 l0,100 l-50,0 l0,-100 Z"
                android:duration="1000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
    <target android:name="four">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M575,500 l50,0 l0,300 l-50,0 l0,-300 Z"
                android:valueTo   =  "M525,540 l50,0 l0,160 l-50,0 l0,-160 Z"
                android:duration="2000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
    <target android:name="five">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:valueFrom =  "M725,400 l50,0 l0,500 l-50,0 l0,-500 Z"
                android:valueTo   =  "M625,440 l50,0 l0,260 l-50,0 l0,-260 Z"
                android:duration="3000"
                android:valueType="pathType"/>
        </aapt:attr>
    </target>
</animated-vector>

