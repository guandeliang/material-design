<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
           <KeyPosition
               motion:motionTarget="@+id/one_image_view"
               motion:framePosition="1"
               motion:percentX="0"
               motion:percentY="0" />
           <KeyPosition
               motion:motionTarget="@+id/one_image_view"
               motion:framePosition="20"
               motion:percentX="1"
               motion:percentY="1" />

           <KeyPosition
               motion:motionTarget="@+id/two_image_view"
               motion:framePosition="1"
               motion:percentX="0"
               motion:percentY="0" />
           <KeyPosition
               motion:motionTarget="@+id/two_image_view"
               motion:framePosition="20"
               motion:percentX="1"
               motion:percentY="1" />

           <KeyPosition
               motion:motionTarget="@+id/one_title_text_view"
               motion:framePosition="20"
               motion:percentX="0"
               motion:percentY="1" />
           <KeyPosition
               motion:motionTarget="@+id/one_title_text_view"
               motion:framePosition="30"
               motion:percentX="1"
               motion:percentY="1" />

           <KeyPosition
               motion:motionTarget="@+id/one_sub_title_text_view"
               motion:framePosition="25"
               motion:percentX="0"
               motion:percentY="1" />
           <KeyPosition
               motion:motionTarget="@+id/one_sub_title_text_view"
               motion:framePosition="35"
               motion:percentX="1"
               motion:percentY="1" />

           <KeyPosition
               motion:motionTarget="@+id/two_sub_title_text_view"
               motion:framePosition="30"
               motion:percentX="0"
               motion:percentY="1" />
           <KeyPosition
               motion:motionTarget="@+id/two_sub_title_text_view"
               motion:framePosition="40"
               motion:percentX="1"
               motion:percentY="1" />

           <KeyPosition
               motion:motionTarget="@+id/two_title_text_view"
               motion:framePosition="35"
               motion:percentX="0"
               motion:percentY="1" />
           <KeyPosition
               motion:motionTarget="@+id/two_title_text_view"
               motion:framePosition="45"
               motion:percentX="1"
               motion:percentY="1" />



           <KeyAttribute
               motion:motionTarget="@+id/color_one_image_view"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_one_image_view"
               motion:framePosition="45"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_one_image_view"
               motion:framePosition="65"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/color_one_image_view"
               motion:framePosition="0"
               android:translationY="48dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_one_image_view"
               motion:framePosition="45"
               android:translationY="48dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_one_image_view"
               motion:framePosition="65"
               android:translationY="0dp" />

           <KeyAttribute
               motion:motionTarget="@+id/color_two_image_view"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_two_image_view"
               motion:framePosition="45"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_two_image_view"
               motion:framePosition="65"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/color_two_image_view"
               motion:framePosition="0"
               android:translationY="24dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_two_image_view"
               motion:framePosition="45"
               android:translationY="24dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_two_image_view"
               motion:framePosition="65"
               android:translationY="0dp" />

           <KeyAttribute
               motion:motionTarget="@+id/color_three_image_view"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_three_image_view"
               motion:framePosition="45"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_three_image_view"
               motion:framePosition="65"
               android:alpha="1" />

           <KeyAttribute
               motion:motionTarget="@+id/color_four_image_view"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_four_image_view"
               motion:framePosition="45"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_four_image_view"
               motion:framePosition="65"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/color_four_image_view"
               motion:framePosition="0"
               android:translationY="-24dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_four_image_view"
               motion:framePosition="45"
               android:translationY="-24dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_four_image_view"
               motion:framePosition="65"
               android:translationY="0dp" />

           <KeyAttribute
               motion:motionTarget="@+id/color_five_image_view"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_five_image_view"
               motion:framePosition="45"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/color_five_image_view"
               motion:framePosition="65"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/color_five_image_view"
               motion:framePosition="0"
               android:translationY="-48dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_five_image_view"
               motion:framePosition="45"
               android:translationY="-48dp" />
           <KeyAttribute
               motion:motionTarget="@+id/color_five_image_view"
               motion:framePosition="65"
               android:translationY="0dp" />




           <KeyPosition
               motion:motionTarget="@+id/top_right_image_view"
               motion:framePosition="65"
               motion:percentX="1"
               motion:percentY="1" />
           <KeyPosition
               motion:motionTarget="@+id/bottom_left_image_view"
               motion:framePosition="75"
               motion:percentX="1"
               motion:percentY="1" />


           <KeyTrigger
               motion:triggerId="@+id/motion_triger_top_right_image_view"
               motion:motionTarget="@+id/top_right_image_view"
               motion:framePosition="65"/>

           <KeyTrigger
               motion:triggerId="@+id/motion_triger_bottom_left_image_view"
               motion:motionTarget="@+id/bottom_left_image_view"
               motion:framePosition="75"/>


           <KeyAttribute
               motion:motionTarget="@+id/footer_text_view"
               motion:framePosition="0"
               android:alpha="0"/>
           <KeyAttribute
               motion:motionTarget="@+id/footer_text_view"
               motion:framePosition="75"
               android:alpha="0"
               android:scaleY="3"/>
           <KeyPosition
               motion:motionTarget="@+id/footer_text_view"
               motion:framePosition="75"
               motion:percentX="1"
               motion:percentY="1" />
           <KeyAttribute
               motion:motionTarget="@+id/footer_text_view"
               motion:framePosition="99"
               android:alpha="1"
               android:scaleX="1"/>

       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/one_image_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio="H,10:16"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintLeft_toLeftOf="@id/left_guide_line"
            motion:layout_constraintRight_toRightOf="@id/v_center_guide_line"/>
        <Constraint
            android:id="@+id/two_image_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio="H,10:16"
            motion:layout_constraintTop_toBottomOf="parent"
            motion:layout_constraintLeft_toLeftOf="@id/v_center_guide_line"
            motion:layout_constraintRight_toRightOf="@id/right_guide_line"/>
        <Constraint
            android:id="@+id/one_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:layout_constraintTop_toTopOf="@id/one_image_view"
            motion:layout_constraintBottom_toTopOf="@id/one_sub_title_text_view"
            motion:layout_constraintLeft_toRightOf="parent"/>
        <Constraint
            android:id="@+id/one_sub_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            motion:layout_constraintTop_toBottomOf="@id/one_title_text_view"
            motion:layout_constraintBottom_toBottomOf="@id/one_image_view"
            motion:layout_constraintLeft_toRightOf="parent"/>
        <Constraint
            android:id="@+id/two_sub_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:layout_constraintTop_toTopOf="@id/two_image_view"
            motion:layout_constraintBottom_toTopOf="@id/two_title_text_view"
            motion:layout_constraintRight_toLeftOf="parent"/>
        <Constraint
            android:id="@+id/two_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            motion:layout_constraintTop_toBottomOf="@id/two_sub_title_text_view"
            motion:layout_constraintBottom_toBottomOf="@id/two_image_view"
            motion:layout_constraintRight_toLeftOf="parent"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/one_image_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio="H,10:16"
            motion:layout_constraintBottom_toTopOf="@id/h_center_guide_line"
            motion:layout_constraintLeft_toLeftOf="@id/left_guide_line"
            motion:layout_constraintRight_toRightOf="@id/v_center_guide_line"/>
        <Constraint
            android:id="@+id/two_image_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio="H,10:16"
            motion:layout_constraintTop_toBottomOf="@id/h_center_guide_line"
            motion:layout_constraintLeft_toLeftOf="@id/v_center_guide_line"
            motion:layout_constraintRight_toRightOf="@id/right_guide_line"/>
        <Constraint
            android:id="@+id/one_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:layout_constraintTop_toTopOf="@id/one_image_view"
            motion:layout_constraintBottom_toTopOf="@id/one_sub_title_text_view"
            motion:layout_constraintLeft_toRightOf="@id/one_image_view"/>
        <Constraint
            android:id="@+id/one_sub_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            motion:layout_constraintTop_toBottomOf="@id/one_title_text_view"
            motion:layout_constraintBottom_toBottomOf="@id/one_image_view"
            motion:layout_constraintLeft_toRightOf="@id/one_image_view"/>
        <Constraint
            android:id="@+id/two_sub_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            motion:layout_constraintVertical_chainStyle="packed"
            motion:layout_constraintTop_toTopOf="@id/two_image_view"
            motion:layout_constraintBottom_toTopOf="@id/two_title_text_view"
            motion:layout_constraintRight_toLeftOf="@id/two_image_view"/>
        <Constraint
            android:id="@+id/two_title_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            motion:layout_constraintTop_toBottomOf="@id/two_sub_title_text_view"
            motion:layout_constraintBottom_toBottomOf="@id/two_image_view"
            motion:layout_constraintRight_toLeftOf="@id/two_image_view" />
    </ConstraintSet>
</MotionScene>